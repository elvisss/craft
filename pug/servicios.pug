    .wrap-process-nav.d-none.d-lg-block
      #services-process-nav
        mixin process(val, index)
          .box-nav-process(class=index % 2 == 0 ? '' : 'box-nav-top')
            .box-nav-number
              span=index >= 9 ? index + 1 : "0" + (index + 1)
            .box-nav-text
              span=val

        //- each val, index in ['conceptualización', 'distribución espacial', 'planimetria arquitectónica', 'selección de acabados', 'ingenierías de detalle', 'ejecución', 'implementación de mobiliano', 'planificación', 'control de calidad', 'cierre', 'conceptualización', 'distribución espacial', 'planimetria arquitectónica', 'selección de acabados', 'ingenierías de detalle', 'ejecución', 'implementación de mobiliano', 'planificación', 'control de calidad', 'cierre']
